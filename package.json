{
  "name": "medapp",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start:hmr": "ng serve --hmr -e=hmr",
    "start:server": "npm run build:all:local && npm run server",
    "build": "ng build",
    "test": "ng test --browsers Chrome",
    "test:ci": "ng test --single-run --browsers ChromeHeadless",
    "test:headless": "ng test --browsers ChromeHeadless",
    "test:server": "jest -c server/jest.config.json",
    "test:server:watch": "npm run test:server -- --watch",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "build:client": "ng build --prod --aot --build-optimizer && ts-node scripts/inline.ts",
    "build:client:local": "ng build --aot --build-optimizer && ts-node scripts/inline.ts",
    "build:server": "ng build --prod --aot --build-optimizer --app 1 --output-hashing=false && webpack --config webpack.server.config.ts --progress --colors",
    "build:server:local": "ng build --aot --build-optimizer --app 1 --output-hashing=false && webpack --config webpack.server.config.ts --progress --colors",
    "build:all": "npm run build:client && npm run build:server",
    "build:all:local": "npm run build:client:local && npm run build:server:local",
    "build:ssr:watch": "concurrently \"ng build --prod --watch\" \"ng build --prod --app 1 --output-hashing=false --watch\" \"webpack --config webpack.server.config.ts --progress --colors --watch\" ",
    "server": "nodemon dist/server.js",
    "serve:ssr": "concurrently \"npm run build:ssr:watch\" \"npm run server\" ",
    "tt": "cd ../mi-node-tests && git commit --allow-empty -m 'trigger' && git push origin master",
    "analyze": "ng build --prod --stats-json && webpack-bundle-analyzer dist/browser/stats.json",
    "firebase:copy": "rm -rf firebase/functions/dist && cp -r dist firebase/functions/ && ts-node scripts/firebase.ts",
    "firebase:deploy": "npm run firebase:copy && cd firebase/functions && ls -al && firebase deploy",
    "firebase": "npm run build:all && npm run firebase:deploy",
    "firebase:server": "npm run build:server && npm run firebase:deploy"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^5.0.0",
    "@angular/common": "^5.0.0",
    "@angular/compiler": "^5.0.0",
    "@angular/core": "^5.0.0",
    "@angular/flex-layout": "^5.0.0-beta.13",
    "@angular/forms": "^5.0.0",
    "@angular/http": "^5.0.0",
    "@angular/platform-browser": "^5.0.0",
    "@angular/platform-browser-dynamic": "^5.0.0",
    "@angular/platform-server": "^5.0.0",
    "@angular/router": "^5.0.0",
    "@nguniversal/module-map-ngfactory-loader": "^5.0.0-beta.5",
    "@types/webpack": "^3.0.13",
    "core-js": "^2.4.1",
    "cors": "^2.8.4",
    "domino": "^2.0.0",
    "express": "^4.16.2",
    "firebase": "^4.6.0",
    "firebase-admin": "^5.4.2",
    "firebase-functions": "^0.8.0",
    "reflect-metadata": "^0.1.10",
    "rxjs": "^5.5.2",
    "sqlite3": "^3.1.13",
    "tslib": "^1.8.0",
    "xhr2": "^0.1.4",
    "zone.js": "^0.8.14"
  },
  "devDependencies": {
    "@angular/cli": "^1.5.4",
    "@angular/compiler-cli": "^5.0.0",
    "@angular/language-service": "^5.0.0",
    "@angularclass/hmr": "^2.1.3",
    "@types/cors": "^2.8.1",
    "@types/jasmine": "~2.8.3",
    "@types/jasminewd2": "~2.0.2",
    "@types/node": "~9.4.0",
    "@types/request": "^2.0.6",
    "@types/sqlite3": "^3.1.1",
    "codelyzer": "~4.1.0",
    "concurrently": "^3.5.0",
    "jasmine-core": "~3.0.0",
    "jasmine-spec-reporter": "~4.2.1",
    "jest": "^22.0.1",
    "karma": "~2.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^1.2.1",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "nodemon": "^1.12.1",
    "protractor": "~5.3.0",
    "request": "^2.83.0",
    "ts-jest": "^22.0.0",
    "ts-loader": "^3.0.5",
    "ts-node": "~5.0.0",
    "tslint": "~5.9.0",
    "typescript": "~2.6.1",
    "webpack-bundle-analyzer": "^2.9.1"
  }
}
