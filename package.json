{
  "name": "medapp",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start:hmr": "ng serve --hmr -e=hmr",
    "start:server": "npm run build:all && npm run server",
    "build": "ng build",
    "test": "ng test",
    "test:server": "jest -c server/jest.config.json",
    "test:server:watch": "npm run test:server -- --watch",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "build:client": "ng build --prod",
    "build:server": "ng build --prod --app 1 --output-hashing=false && webpack --config webpack.server.config.ts --progress --colors",
    "build:all": "npm run build:client && npm run build:server",
    "build:ssr:watch": "concurrently \"ng build --prod --watch\" \"ng build --prod --app 1 --output-hashing=false --watch\" \"webpack --config webpack.server.config.ts --progress --colors --watch\" ",
    "server": "nodemon dist/server.js",
    "serve:ssr": "concurrently \"npm run build:ssr:watch\" \"npm run server\" ",
    "tt": "cd ../mi-node-tests && git commit --allow-empty -m 'trigger' && git push origin master"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^5.0.0-rc.3",
    "@angular/common": "^5.0.0-rc.3",
    "@angular/compiler": "^5.0.0-rc.3",
    "@angular/core": "^5.0.0-rc.3",
    "@angular/forms": "^5.0.0-rc.3",
    "@angular/http": "^5.0.0-rc.3",
    "@angular/platform-browser": "^5.0.0-rc.3",
    "@angular/platform-browser-dynamic": "^5.0.0-rc.3",
    "@angular/platform-server": "^5.0.0-rc.3",
    "@angular/router": "^5.0.0-rc.3",
    "@nguniversal/module-map-ngfactory-loader": "^1.0.0-beta.3",
    "@types/webpack": "^3.0.13",
    "core-js": "^2.4.1",
    "cors": "^2.8.4",
    "firebase": "^4.6.0",
    "firebase-admin": "^5.4.2",
    "firebase-functions": "^0.7.1",
    "rxjs": "^5.4.2",
    "sqlite3": "^3.1.13",
    "zone.js": "^0.8.14"
  },
  "devDependencies": {
    "@angular/cli": "1.4.9",
    "@angular/compiler-cli": "^5.0.0-rc.3",
    "@angular/language-service": "^5.0.0-rc.3",
    "@angularclass/hmr": "^2.1.3",
    "@types/cors": "^2.8.1",
    "@types/jasmine": "~2.6.1",
    "@types/jasminewd2": "~2.0.2",
    "@types/node": "~8.0.47",
    "@types/request": "^2.0.6",
    "@types/sqlite3": "^3.1.1",
    "codelyzer": "~4.0.0",
    "concurrently": "^3.5.0",
    "jasmine-core": "~2.8.0",
    "jasmine-spec-reporter": "~4.2.1",
    "jest": "^21.2.1",
    "karma": "~1.7.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^1.2.1",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "nodemon": "^1.12.1",
    "protractor": "~5.2.0",
    "request": "^2.83.0",
    "ts-jest": "^21.1.3",
    "ts-loader": "^3.0.5",
    "ts-node": "~3.3.0",
    "tslint": "~5.8.0",
    "typescript": "~2.6.1"
  }
}
